        <attr name="pull_dotRadius" format="dimension" />
        <attr name="pull_useMultipleColors" format="boolean" />
        <attr name="pull_dotColors" format="reference" />
        <attr name="pull_dotColor" format="color" />
        <attr name="pull_radius" format="dimension" />
        <attr name="pull_animDuration" format="integer" />
    </declare-styleable>


</resources>