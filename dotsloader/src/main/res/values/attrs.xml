        <attr name="fidget_secondDotColor" format="color" />
        <attr name="fidget_thirdDotColor" format="color" />
        <attr name="fidget_drawOnlyStroke" format="boolean" />
        <attr name="fidget_strokeWidth" format="dimension" />
        <attr name="fidget_animDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="LightsLoader">
        <attr name="lights_size" format="integer" />
        <attr name="lights_dotRadius" format="dimension" />
        <attr name="lights_spacing" format="dimension" />
        <attr name="lights_dotColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SlidingLoader">
        <attr name="sliding_dotRadius" format="dimension" />
