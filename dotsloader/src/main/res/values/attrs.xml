<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimationView">
        <attr name="view_toggleOnVisibilityChange" format="boolean" />
    </declare-styleable>

    <declare-styleable name="AnimationLayout">
        <attr name="layout_toggleOnVisibilityChange" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DotView">
        <attr name="dotRadius" format="dimension" />
        <attr name="dotColor" format="color" />
        <attr name="dotDrawOnlyStroke" format="boolean" />
        <attr name="dotStrokeWidth" format="dimension" />
        <attr name="dotIsAntiAlias" format="boolean" />
    </declare-styleable>

    <declare-styleable name="BounceLoader">
        <attr name="bounce_ballRadius" format="dimension" />
        <attr name="bounce_ballColor" format="color" />
        <attr name="bounce_shadowColor" format="color" />
        <attr name="bounce_showShadow" format="boolean" />
        <attr name="bounce_animDuration" format="integer" />
    </declare-styleable>

    <declare-styleable name="ClassicLoader">
        <attr name="classic_inactiveColor" format="color" />
        <attr name="classic_activeColor" format="color" />
        <attr name="classic_dotRadius" format="dimension" />
        <attr name="classic_animDuration" format="integer" />
        <attr name="classic_showRunningShadow" format="boolean" />
        <attr name="classic_firstShadowColor" format="color" />
        <attr name="classic_secondShadowColor" format="color" />
        <attr name="classic_radius" format="dimension" />
